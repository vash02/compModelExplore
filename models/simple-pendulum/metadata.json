{
  "model_name": "Simple Pendulum",
  "equations": "theta'' + (g/L)*sin(theta) = 0",
  "initial_conditions": [
    "theta=3.1416/2",
    "theta'=0"
  ],
  "parameters": {
    "L": "length of pendulum",
    "g": "acceleration due to gravity"
  },
  "vary_variable": "L",
  "objective": "determine relationship between L and period"
}